        "k":"393", //[W/(m*K)]
        "Cp":"386.e+06", //[J/(kg/K)]
        "rho":"8.94e-09" //[kg/(m^3)]
    },
    "Materials":
    {
        "Omega":
        {
            "name":"mymat1",
            "physics":["heat"],
            "k":"393", //[W/(m*K)]
            "Cp":"386.e+06", //[J/(kg/K)]
            "rho":"8.94e-09" //[kg/(m^3)]
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "SourceTerm":
            {
                "Omega":
                {
                    "expr":"y*z*((386.e+06)*(8.94e-09)*x*x-393*2*t):x:y:z:t"
                }
            },
            "Dirichlet":
            {
                "Dirichlet":
                {
                    "expr":"x*x*y*z*t:x:y:z:t"
                }
            },
            "Robin":
            {
                "Robin":
                {
                    "expr1":"4.35e-03",
                    "expr2":"x*x*y*z*t + (393/4.35e-03)*(2*x*y*z*t*nx+x*x*z*t*ny+x*x*y*t*nz):x:y:z:t:nx:ny:nz"
                }
            }
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["temperature"]
        }
    }
}