{
    "Name": "Heatonly",
    "ShortName":"HO",
    "Models":"mythermicmodel",
    "Parameters":
    {
        "k":"393", //[W/(m*K)]
        "Cp":"386.Oe06", //[J/(kg/K)]
        "rho":"8.94e-09" //[kg/(m^3)]
    },
    "Materials":
    {
        "Omega":
        {
            "name":"mymat1",
            "physics":["heat"],
            "k":"k", //[W/(m*K)]
            "Cp":"Cp", //[J/(kg/K)]
            "rho":"rho" //[kg/(m^3)]
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "VolumicForces":
            {
                "Omega":
                {
                    "expr":"y*z*(Cp*rho*x*x-2*t):x:y:z:t"
                }
            },
            "Dirichlet":
            {
                "Dirichlet":
                {
                    "expr":"x*x*y*z*t:x:y:z:t"
                }
            },
            "Neumann":
            {
                "Neumann":
                {
                    "expr":"-x*x*y*t:x:y:z:t"
                }
            },
            "Robin":
            {
                "Robin":
                {
                    "expr1":"4.35e-03", //hcoef
                    "expr2":"x*x*y*t*(k+4.35e-03)/4.35e-03" //temp ext
                }
            }

        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["temperature"]
        }
    }
}