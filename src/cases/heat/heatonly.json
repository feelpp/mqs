        "k":"393", //[W/(m*K)]
        "Cp":"386.e+06", //[J/(kg/K)]
        "rho":"8.94e-09" //[kg/(m^3)]
    },
    "Materials":
    {
        "Omega":
        {
            "name":"mymat1",
            "physics":["heat"],
            "k":"393", //[W/(m*K)]
            "Cp":"386.e+06", //[J/(kg/K)]
            "rho":"8.94e-09" //[kg/(m^3)]
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "SourceTerm":
            {
                "Omega":
                {
                    "expr":"y*z*((386.e+06)*(8.94e-09)*x*x-393*2*t):x:y:z:t"
                }
            },
            "Dirichlet":
            {
                "Dirichlet":
                {
                    "expr":"x*x*y*z*t:x:y:z:t"
                }
            },
            "Neumann":
            {
                "Neumann1":
                {
                    "expr":"x*x*z*t*393:x:y:z:t"
                },
                "Neumann2":
                {
                    "expr":"-2*x*y*z*t*393:x:y:z:t"
                },
                "Neumann3":
                {
                    "expr":"-x*x*z*t*393:x:y:z:t"
                },
                "Neumann4":
                {
                    "expr":"2*x*y*z*t*393:x:y:z:t"
                },
                "Neumann5":
                {
                    "expr":"-x*x*y*t*393:x:y:z:t"
                }
            }
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["temperature"]
        }
    }
}